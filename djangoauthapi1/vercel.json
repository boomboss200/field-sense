{
    "builds": [{
        "src": "djangoauthapi1/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.9" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static"
    }],
    "routes": [
        {
            "src": "/staticfiles/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "djangoauthapi1/wsgi.py"
        }
    ]
}